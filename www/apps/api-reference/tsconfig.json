{
  "extends": "tsconfig/nextjs.json",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "typeRoots": [
      "../../node_modules/@types",
      "./node_modules/@types",
      "./types"
    ],
    "jsx": "preserve",
  },
  "include": [
    "next-env.d.ts",
    "**/*.tsx",
    "**/*.ts",
    "**/*.js",
    ".next/types/**/*.ts",
    "**/*.mjs"
  ],
  "exclude": [
    "specs"
  ]
}
